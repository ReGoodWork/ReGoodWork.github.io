    <app>
        <name>Microsoft Solitaire Collection</name>
        <category>Games</category>
        <icon>https://microsoftwarestudios.github.io/winstore/appRes/solitaire/StoreLogo.png</icon>
        <description>
            #1 Top Free Game in the Windows Store! Pick up and play 
            the evolution of this classic card game on Windows 8!
            Microsoft Solitaire Collection features all of your favorite 
            game modes: Klondike, FreeCell, Spider, TriPeaks, and 
            Pyramid. Play Daily Challenges, choose themes and take 
            your game online with Xbox achievements and 
            leaderboards. 
        </description>
        <features>
            <feature>Klondike (Classic Solitaire)</feature>
            <feature>Spider Solitaire</feature>
            <feature>FreeCell</feature>
            <feature>Pyramid</feature>
            <feature>TriPeaks</feature>
            <feature>Multiple themes</feature>
            <feature>Daily Challenges!</feature>
        </features>
        <install>https://microsoftwarestudios.github.io/winstore/install/mssolitaire.ps1</install>
        <publisher>Microsoft Studios</publisher>
    </app>
